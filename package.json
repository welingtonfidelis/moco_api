{
  "name": "moco_api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "start": "node dist/server.js",
    "postinstall": "tsc",
    "deploy": "git add . && git commit -m Heroku && git push heroku main",
    "dev": "ts-node-dev --respawn --transpile-only --ignore-watch node_modules src/server.ts",
    "migrate:generate": "sequelize-cli migration:generate --name",
    "migrate:apply": "sequelize-cli db:migrate",
    "migrate:revert": "sequelize-cli db:migrate:undo",
    "migrate:revert:all": "sequelize-cli db:migrate:undo:all",
    "seed:apply": "sequelize-cli db:seed:all",
    "seed:revert": "sequelize-cli db:seed:undo:all"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/node": "^15.0.2",
    "@types/pdfkit": "^0.11.0",
    "@types/validator": "^13.1.3",
    "ts-node-dev": "^1.1.6"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.4.4",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^9.0.1",
    "express": "^4.17.1",
    "handlebars": "^4.7.7",
    "intl": "^1.2.5",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "pdfkit": "^0.12.1",
    "pg": "^8.6.0",
    "sequelize": "^6.6.2",
    "sequelize-cli": "^6.2.0",
    "typescript": "^4.2.4"
  }
}
